//Maximo Login Logout Test
	public void Maximo_login_Test() throws IOException {
		enterTextField(By.id(ObjectRepo.sUsername_id), "CM-Usupervisor");
		enterTextField(By.id(or.sPassword_id), "Maximo.75#");
		clickButton(By.id(or.sLogin_id));
		Wait(2);
		Assert.assertEquals(driver.getTitle(), "Start Center");
		clickLink(By.cssSelector(or.sSignOut_CSS));
		Wait(3);
		clickButton(By.id("submit"));
		Assert.assertTrue(driver.getTitle().contains("Welcome to Maximo ETown"));
		log.info(Thread.currentThread().getStackTrace()[1].getMethodName() + " ****Execution Completed");
	}

